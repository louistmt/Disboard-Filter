{
  "name": "disboard-filter",
  "version": "0.0.1",
  "description": "A small extension to filter out already seen servers on disboard",
  "main": "index.mjs",
  "scripts": {
    "lint": "web-ext lint --source-dir=extension",
    "start": "web-ext run --source-dir=extension --verbose",
    "clean": "del build\\*.zip",
    "zip": "web-ext build --source-dir=extension --artifacts-dir=build",
    "build": "npm run clean && npm run zip",
    "test": "web-ext --version"
  },
  "keywords": [
    "extension",
    "disboard"
  ],
  "author": "louistmt",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@types/firefox-webext-browser": "^111.0.1",
    "web-ext": "^7.6.2"
  }
}
